{
    "name": "ai-testing-agent-vscode",
    "displayName": "AI Testing Agent",
    "description": "Integrates with AI Testing Agent API to automatically test webpages and fix issues",
    "version": "0.0.1",
    "publisher": "ai-testing-agent",
    "engines": {
        "vscode": "^1.85.0"
    },
    "categories": [
        "Testing",
        "Machine Learning"
    ],
    "activationEvents": [
        "onCommand:ai-testing-agent.testPage",
        "onCommand:ai-testing-agent.fixIssue"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "ai-testing-agent.testPage",
                "title": "AI Testing Agent: Test Webpage"
            },
            {
                "command": "ai-testing-agent.fixIssue",
                "title": "AI Testing Agent: Fix Issue"
            }
        ],
        "configuration": {
            "title": "AI Testing Agent",
            "properties": {
                "aiTestingAgent.apiUrl": {
                    "type": "string",
                    "default": "http://localhost:3002",
                    "description": "URL of the IDE Testing API"
                },
                "aiTestingAgent.apiKey": {
                    "type": "string",
                    "default": "",
                    "description": "API Key for authentication"
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "package": "vsce package"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/vscode": "^1.85.0",
        "typescript": "^5.3.3",
        "@vscode/test-electron": "^2.3.8"
    },
    "dependencies": {
        "axios": "^1.6.2"
    }
}